{
  "name": "usdm-ts",
  "version": "1.0.0",
  "type": "module",
  "description": "A Typescript library that provides a structured representation of the USDM (Unified Study Data Model) for clinical trials.",
  "scripts": {
    "build": "tsc",
    "generate": "tsx src/generate.ts"
  },
  "keywords": [
    "usdm",
    "clinical-trials",
    "cdisc",
    "ddf"
  ],
  "author": "HumanTrue Inc.",
  "license": "Apache-2.0",
  "packageManager": "pnpm@10.12.1",
  "engines": {
    "pnpm": ">=10.12.1",
    "npm": "please-use-pnpm"
  },
  "exports": [
    "./src/generated/types.ts",
    "./src/generated/zod.ts"
  ],
  "dependencies": {
    "@types/node": "^24.0.15",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "yaml": "^2.8.0",
    "zod": "^4.0.5"
  }
}
